{
  "name": "<%= pkgName %>",
  "version": "1.0.0",
  "boneVersion": "1.2.11",
  "private": true,
  "scripts": {
    "prepare": "npm config set registry http://registry.npm.taobao.org/ && npm config set sass_binary_site https://npm.taobao.org/mirrors/node-sass/",
    "deprepare": "npm config delete registry && npm config delete sass_binary_site",
    "build": "gulp build",
    "dev": "gulp serve",
    "prod": "NODE_ENV=prod node --harmony ./.bone/serve.js",
    "test": "NODE_ENV=test node --harmony ./.bone/serve.js",
    "forever-dev": "export NODE_ENV=dev && forever start --uid ${PWD##*/} --append -c \"node --harmony\" ./.bone/serve.js",
    "forever-prod": "export NODE_ENV=prod && forever start --uid ${PWD##*/} --append -c \"node --harmony\" ./.bone/serve.js",
    "forever-test": "export NODE_ENV=test && forever start --uid ${PWD##*/} --append -c \"node --harmony\" ./.bone/serve.js",
    "forever-restart": "forever restart ${PWD##*/}",
    "forever-stop": "forever stop ${PWD##*/}",
    "pm2-start": "export NODE_ENV=prod && pm2 start ./.bone/serve.js --name=\"${PWD##*/}\" ",
    "pm2-start-cluster": "export NODE_ENV=prod && pm2 start ./.bone/serve.js --name=\"${PWD##*/}\" -i 1",
    "pm2-reload": "export NODE_ENV=prod && pm2 reload ${PWD##*/}",
    "pm2-restart": "export NODE_ENV=prod && pm2 restart ${PWD##*/}",
    "pm2-stop": "export NODE_ENV=prod && pm2 stop ${PWD##*/}",
    "pm2-delete": "export NODE_ENV=prod && pm2 delete ${PWD##*/}",
    "upgrade": "npm update -g generator-bone && yo bone:update"
  },
  "engines": {
    "node": "4.4.7"
  },
